{
    "name": "lxdhub",
    "version": "0.9.0",
    "description": "Display, search and copy LXD-images using a web interface.",
    "scripts": {
        "start": "npm run start:api",
        "lint:api": "tslint -c tslint.json 'src/api/*.ts'",
        "lint:db": "tslint -c tslint.json 'src/db/*.ts'",
        "lint:common": "tslint -c tslint.json 'src/common/*.ts'",
        "lint:dbsync": "tslint -c tslint.json 'src/dbsync/*.ts'",
        "lint": "npm run lint:api && npm run lint:db && npm run lint:dbsync && npm run lint:common",
        "start:api": "node bin/start-api.js",
        "start:dbsync": "node bin/start-dbsync.js",
        "build": "node node_modules/gulp/bin/gulp.js build",
        "build:lib": "node node_modules/gulp/bin/gulp.js build --dist lib",
        "postinstall": "postinstall-build node_modules/@lxdhub",
        "docs": "node bin/generate-graphs.js && tsviz src/db/models/ lib/db/docs/assets/graphs/db-schema.png",
        "test": "jest --config=jest.json -i",
        "test:watch": "jest --watch --config=jest.json -i",
        "test:dev": "jest --watch --config=jest.json -i --coverage --coverageDirectory=coverage",
        "test:coverage": "jest --config=jest.json -i --coverage --coverageDirectory=coverage",
        "dev:api": "node node_modules/gulp/bin/gulp.js api:serve --script bin/start-api.js",
        "dev:dbsync": "node node_modules/gulp/bin/gulp.js dbsync:serve --script bin/start-dbsync.js"
    },
    "dependencies": {
        "@nestjs/common": "^4.6.6",
        "@nestjs/core": "^4.6.6",
        "@nestjs/microservices": "^4.6.6",
        "@nestjs/swagger": "^1.2.2",
        "@nestjs/testing": "^4.6.6",
        "@nestjs/typeorm": "2.0.0",
        "@nestjs/websockets": "^4.6.6",
        "aigle": "^1.12.0-alpha.6",
        "clime": "^0.5.9",
        "commander": "^2.15.0",
        "fs-promise": "^2.0.3",
        "gulp-sourcemaps": "^2.6.4",
        "js-yaml": "^3.11.0",
        "lodash": "^4.17.5",
        "node-lxd-client": "0.0.7",
        "pg": "^7.4.1",
        "redis": "^2.8.0",
        "reflect-metadata": "^0.1.12",
        "request": "^2.85.0",
        "request-promise": "^4.2.2",
        "rxjs": "^5.5.8",
        "socket.io": "^2.1.0",
        "tslint": "^5.9.1",
        "typeorm": "^0.2.0-alpha.28",
        "typescript": "^2.8.1",
        "winston": "^3.0.0-rc5"
    },
    "devDependencies": {
        "@types/express": "^4.11.1",
        "@types/fs-promise": "^1.0.3",
        "@types/jest": "^22.2.2",
        "@types/js-yaml": "^3.11.1",
        "@types/lodash": "^4.14.106",
        "@types/node": "^9.3.0",
        "@types/socket.io": "^1.4.33",
        "gulp": "^3.9.1",
        "gulp-cli": "^2.0.1",
        "gulp-file-cache": "0.0.1",
        "gulp-nodemon": "^2.2.1",
        "gulp-sequence": "^1.0.0",
        "gulp-typescript": "^4.0.2",
        "jest": "^22.4.3",
        "lerna": "^2.9.1",
        "madge": "^3.0.1",
        "nodemon": "^1.17.3",
        "postinstall-build": "^5.0.1",
        "sqlite3": "^3.1.13",
        "supertest": "^3.0.0",
        "ts-jest": "^22.4.2",
        "ts-node": "^5.0.1",
        "tsviz": "^1.0.11"
    },
    "license": "Apache-2.0",
    "keywords": [
        "lxd",
        "lxc",
        "lxdhub",
        "container"
    ],
    "homepage": "https://github.com/Roche/lxdhub#README.md",
    "bugs": {
        "url": "https://github.com/Roche/lxdhub/issues",
        "email": "livio.brunner.lb1@roche.com"
    },
    "author": {
        "email": "livio.brunner.lb1@roche.com",
        "name": "Livio Brunner",
        "url": "https://brunnerliv.io"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Roche/lxdhub"
    }
}
